@model Bluepill.Web.Areas.Administration.Models.SearchModel

@{ Layout = null; }


<div id="results" data-page="@Model.Page" data-max="@Model.TotalPages" data-boxes="@Model.TotalBoxes">

    @for (int i = 0; i <= Model.MaxIndex; i++)
    {
        <div class="match">
            <a  href="~/application/picture/getpicturebytes?index=@i" target="_blank">
                <img src="~/application/picture/getpicturereducedbytes?index=@i" />
            </a>
        </div>
    }

    <div class="trash"><img src="\content\images\recycle.png" /></div>
</div>
