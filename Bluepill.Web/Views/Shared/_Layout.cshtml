<!DOCTYPE html>
<html>
<head>
    <title>Blue Pill</title>
    <link href="/content/themes/base/jquery.ui.all.css" rel="stylesheet" type="text/css" />
    <link href="/content/themes/sunny/jquery-ui-1.8.23.custom.css")" type="text/css" rel="stylesheet" />
    <link href="/content/page.css" rel="stylesheet" type="text/css" />
    <link href="/content/application.search.interface.css" rel="stylesheet" type="text/css" />
    @if (IsSectionDefined("css"))
    {
        @RenderSection("css")
    }
    
    <script src="/scripts/jquery-1.8.1.min.js" type="text/javascript"></script>
    <script src="/scripts/jquery-ui-1.8.23.min.js" type="text/javascript"></script>
    @if (IsSectionDefined("script"))
    {
        @RenderSection("script")
    }

</head>
<body data-navigation-index="@ViewBag.NavigationIndex">
    <div class="page">
           
         <div class="content content-gradient">
            @if (IsSectionDefined("settings"))
            {
                @RenderSection("settings")
            }
            else
            {
                @Html.Partial("/Areas/Application/Views/Settings/Index.cshtml")
            }
             @RenderBody()
         </div>
         
    </div>
    
</body>
</html>

